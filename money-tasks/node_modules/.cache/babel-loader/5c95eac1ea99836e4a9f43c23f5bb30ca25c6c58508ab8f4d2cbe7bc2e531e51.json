{"ast":null,"code":"import { ref, reactive, computed, watch, watchEffect } from \"vue\";\nexport default {\n  props: {\n    theme: {\n      type: String,\n      required: false,\n      default: \"light\"\n    }\n  },\n  setup(props, context) {\n    const searchText = ref(\"\");\n    const customers = reactive([\"Something\", \"Tien Le\", \"Sky Albert\", \"Hula\"]);\n    const customersFiltered = computed(() => customers.map(customer => {\n      customer = customer.toLowerCase();\n      return customer;\n    }).filter(customer => customer.includes(searchText.value.toLocaleUpperCase())));\n    watch(searchText, (newValue, oldValue) => {\n      console.log(newValue, oldValue);\n    });\n    watchEffect(() => {\n      if (searchText.value) {\n        console.log(\"run again\");\n      }\n    });\n    return {\n      searchText,\n      customersFiltered\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","computed","watch","watchEffect","props","theme","type","String","required","default","setup","context","searchText","customers","customersFiltered","map","customer","toLowerCase","filter","includes","value","toLocaleUpperCase","newValue","oldValue","console","log"],"sources":["D:\\WorkSpace\\Vue3\\money-tasks\\src\\pages\\home.vue"],"sourcesContent":["<template>\r\n  <h1>Overview Page is here</h1>\r\n  <input type=\"text\" v-model=\"searchText\" />\r\n  <ul>\r\n    <li v-for=\"customer in customersFiltered\">\r\n      {{ customer }}\r\n    </li>\r\n  </ul>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive, computed, watch, watchEffect } from \"vue\";\r\nexport default {\r\n  props: {\r\n    theme: {\r\n      type: String,\r\n      required: false,\r\n      default: \"light\",\r\n    },\r\n  },\r\n  setup(props, context) {\r\n    const searchText = ref(\"\");\r\n    const customers = reactive([\"Something\", \"Tien Le\", \"Sky Albert\", \"Hula\"]);\r\n\r\n    const customersFiltered = computed(() =>\r\n      customers\r\n        .map((customer) => {\r\n          customer = customer.toLowerCase();\r\n          return customer;\r\n        })\r\n        .filter((customer) =>\r\n          customer.includes(searchText.value.toLocaleUpperCase())\r\n        )\r\n    );\r\n\r\n    watch(searchText, (newValue, oldValue) => {\r\n      console.log(newValue, oldValue);\r\n    });\r\n\r\n    watchEffect(() => {\r\n      if (searchText.value) {\r\n        console.log(\"run again\");\r\n      }\r\n    });\r\n\r\n    return { searchText, customersFiltered };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAWA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,QAAQ,EAAEC,KAAK,EAAEC,WAAU,QAAS,KAAK;AACjE,eAAe;EACbC,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE,KAAK;MACfC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,KAAKA,CAACN,KAAK,EAAEO,OAAO,EAAE;IACpB,MAAMC,UAAS,GAAIb,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMc,SAAQ,GAAIb,QAAQ,CAAC,CAAC,WAAW,EAAE,SAAS,EAAE,YAAY,EAAE,MAAM,CAAC,CAAC;IAE1E,MAAMc,iBAAgB,GAAIb,QAAQ,CAAC,MACjCY,SAAQ,CACLE,GAAG,CAAEC,QAAQ,IAAK;MACjBA,QAAO,GAAIA,QAAQ,CAACC,WAAW,CAAC,CAAC;MACjC,OAAOD,QAAQ;IACjB,CAAC,EACAE,MAAM,CAAEF,QAAQ,IACfA,QAAQ,CAACG,QAAQ,CAACP,UAAU,CAACQ,KAAK,CAACC,iBAAiB,CAAC,CAAC,CACxD,CACJ,CAAC;IAEDnB,KAAK,CAACU,UAAU,EAAE,CAACU,QAAQ,EAAEC,QAAQ,KAAK;MACxCC,OAAO,CAACC,GAAG,CAACH,QAAQ,EAAEC,QAAQ,CAAC;IACjC,CAAC,CAAC;IAEFpB,WAAW,CAAC,MAAM;MAChB,IAAIS,UAAU,CAACQ,KAAK,EAAE;QACpBI,OAAO,CAACC,GAAG,CAAC,WAAW,CAAC;MAC1B;IACF,CAAC,CAAC;IAEF,OAAO;MAAEb,UAAU;MAAEE;IAAkB,CAAC;EAC1C;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}