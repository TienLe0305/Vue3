{"ast":null,"code":"import { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nexport default {\n  setup() {\n    const store = useStore();\n    store.dispatch(\"fetchAllTransaction\");\n    const transactions = computed(() => store.state.transactions);\n    const error = computed(() => store.state.error);\n    return {\n      transactions,\n      error\n    };\n  }\n};","map":{"version":3,"names":["computed","useStore","setup","store","dispatch","transactions","state","error"],"sources":["D:\\WorkSpace\\Vue3\\money-tasks\\src\\pages\\transactions.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"transactions.length\">\r\n    <div class=\"item\" v-for=\"transaction in transactions\" :key=\"transaction.id\">\r\n      <router-link\r\n        :to=\"{\r\n          name: 'transaction-details-route',\r\n          params: { id: transaction.id },\r\n        }\"\r\n        >{{ transaction.name }}</router-link\r\n      >\r\n      <div class=\"price\">{{ transaction.price }}</div>\r\n    </div>\r\n  </div>\r\n  <div v-else-if=\"error\">{{ error }}</div>\r\n  <div v-else>Loading Transaction ...</div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from \"vue\";\r\nimport { useStore } from \"vuex\";\r\nexport default {\r\n  setup() {\r\n    const store = useStore();\r\n\r\n    store.dispatch(\"fetchAllTransaction\");\r\n\r\n    const transactions = computed(() => store.state.transactions);\r\n    const error = computed(() => store.state.error);\r\n    return { transactions, error };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAkBA,SAASA,QAAO,QAAS,KAAK;AAC9B,SAASC,QAAO,QAAS,MAAM;AAC/B,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIF,QAAQ,CAAC,CAAC;IAExBE,KAAK,CAACC,QAAQ,CAAC,qBAAqB,CAAC;IAErC,MAAMC,YAAW,GAAIL,QAAQ,CAAC,MAAMG,KAAK,CAACG,KAAK,CAACD,YAAY,CAAC;IAC7D,MAAME,KAAI,GAAIP,QAAQ,CAAC,MAAMG,KAAK,CAACG,KAAK,CAACC,KAAK,CAAC;IAC/C,OAAO;MAAEF,YAAY;MAAEE;IAAM,CAAC;EAChC;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}